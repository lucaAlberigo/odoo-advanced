<odoo>
	<data>

		<record model="ir.ui.view" id="cars_components">
			<field name="name">cars components</field>
			<field name="model">cars.components</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<group>
								<field name="name"/>
							</group>
							<group>
								<field name="quantity"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="cars_components_tree">
			<field name="name">cars components</field>
			<field name="model">cars.components</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
					<field name="quantity"/>
				</tree>
			</field>
		</record>

	</data>
</odoo>
